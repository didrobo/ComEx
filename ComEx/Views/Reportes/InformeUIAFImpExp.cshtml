
@{
    ViewBag.Title = "Informe UIAF Imp Exp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css
{
    <link href="@Url.Content("~/bower_components/datatables/extensions/Buttons/css/buttons.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/datatables/extensions/ColReorder/css/colReorder.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/pages/uiaf.imp.exp.css")" rel="stylesheet" type="text/css" />
}

<div class="row" id="page-hd">
    <div class="col-lg-12">
        <h4 class="page-header">Informe UIAF Imp Exp</h4>
    </div>
</div>

<div id="Principal">

    <div class="row">
        <div class='col-sm-3'></div>
        <div class='col-sm-2'>
            <div class="form-group form-group-lg">
                <label for="intAno">Año</label>
                <input id="intAno" name="intAno" class="form-control" type="number" min="1900" max="2099" step="1" value="@DateTime.Now.Year" />
                <span class="field-validation-valid text-danger" data-valmsg-for="intAno" data-valmsg-replace="true" id="valid_intAno"></span>
            </div>
        </div>
        <div class='col-sm-3'>
            <div class="form-group form-group-lg">
                <label for="intCuatrimestre">Cuatrimestre</label>
                <select id="intCuatrimestre" name="intCuatrimestre" class="form-control">
                    <option value="1" selected>1 Cuatrimestre</option>
                    <option value="2">2 Cuatrimestre</option>
                    <option value="3">3 Cuatrimestre</option>
                </select>
                <span class="field-validation-valid text-danger" data-valmsg-for="intCuatrimestre" data-valmsg-replace="true" id="valid_intCuatrimestre"></span>
            </div>
        </div>
        <div class='col-sm-2'>
            <div class="form-group">
                <br />
                <button type="submit" role="button" class="btn btn-lg btn-primary" id="btnProcesar" data-loading-text="Procesando Informe UIAF Imp-Exp...">Procesar</button>
            </div>
        </div>
        <div class='col-sm-2'></div>
    </div>

</div>

<div id="Informacion" class="ocultar">
    <div class="row">
        <div class="col-lg-12">
            <div class="dataTable_wrapper">
                <table class="table table-striped table-bordered table-hover display compact" style="border-spacing: 0px; border-collapse: separate; width: 100%;" id="table-UIAFImpExp">
                    <thead></thead>
                    <tbody></tbody>
                    <tfoot>
                        <tr>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script src="~/bower_components/bootstrap-modal-wrapper/bootstrap.modal.wrapper.js"></script>
    <script src="~/bower_components/datatables/extensions/Buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/bower_components/datatables/extensions/Buttons/js/jszip.js"></script>
    <script src="~/bower_components/datatables/extensions/Buttons/js/buttons.html5.min.js"></script>
    <script src="~/bower_components/datatables/extensions/ColReorder/js/dataTables.colReorder.min.js"></script>
    <script src="~/bower_components/datatables/extensions/FormFilter/js/dataTables.formFilter.js"></script>
    <script src="~/Scripts/pages/uiaf.imp.exp.js"></script>
}