
@{
    ViewBag.Title = "Inventario CPs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css
{
    <link href="@Url.Content("~/bower_components/datatables/extensions/Buttons/css/buttons.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/datatables/extensions/ColReorder/css/colReorder.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/bootstrap-datetimepicker/css/bootstrap-datetimepicker.css")" media="all" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/bower_components/pages/inventario.cps.css")" rel="stylesheet" type="text/css" />
}

<div class="row" id="page-hd">
    <div class="col-lg-12">
        <h4 class="page-header">Inventario CPs</h4>
    </div>
</div>

<div id="Principal">
    <div class="container" style="position: relative;">
        <div class="row">
            <div class='col-sm-3'></div>
            <div class='col-sm-2'>
                <div class="form-group form-group-lg">
                    <label for="fecDesde">Desde</label>
                    <input id="fecDesde" name="fecDesde" class="form-control calendario" type="datetime" />
                    <span class="field-validation-valid text-danger" data-valmsg-for="fecDesde" data-valmsg-replace="true" id="valid_fecDesde"></span>
                </div>
            </div>
            <div class='col-sm-2'>
                <div class="form-group form-group-lg">
                    <label for="fecHasta">Hasta</label>
                    <input id="fecHasta" name="fecDesde" class="form-control calendario" type="datetime" />
                    <span class="field-validation-valid text-danger" data-valmsg-for="fecHasta" data-valmsg-replace="true" id="valid_fecHasta"></span>
                </div>
            </div>
            <div class='col-sm-2'>
                <div class="form-group">
                    <br />
                    <button type="submit" role="button" class="btn btn-lg btn-primary" id="btnProcesar" data-loading-text="Procesando Inventario CPs...">Procesar</button>
                </div>
            </div>
            <div class='col-sm-3'></div>
        </div>
    </div>
</div>

    <div id="Informacion" class="ocultar">
        <div class="row">
            <div class="col-lg-12">
                <div class="dataTable_wrapper">
                    <table class="table table-striped table-bordered table-hover display compact" style="border-spacing: 0px; border-collapse: separate; width: 100%;" id="table-InventarioCPs">
                        <thead></thead>
                        <tbody></tbody>
                        <tfoot>
                            <tr>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                                <th></th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>


    @section scripts
{
        <script src="~/bower_components/bootstrap-modal-wrapper/bootstrap.modal.wrapper.js"></script>
        <script src="~/bower_components/datatables/extensions/Buttons/js/dataTables.buttons.min.js"></script>
        <script src="~/bower_components/datatables/extensions/Buttons/js/jszip.js"></script>
        <script src="~/bower_components/datatables/extensions/Buttons/js/buttons.html5.min.js"></script>
        <script src="~/bower_components/datatables/extensions/ColReorder/js/dataTables.colReorder.min.js"></script>
        <script src="~/bower_components/datatables/extensions/FormFilter/js/dataTables.formFilter.js"></script>
        <script src="~/bower_components/bootstrap-datetimepicker/js/moment.min.js"></script>
        <script src="~/bower_components/bootstrap-datetimepicker/js/moment-with-locales.js"></script>
        <script src="~/bower_components/bootstrap-datetimepicker/js/bootstrap-datetimepicker.js"></script>
        <script src="~/Scripts/pages/inventario.cps.js"></script>
    }
